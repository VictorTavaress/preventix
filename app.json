{
  "expo": {
    "name": "preventix",
    "slug": "preventix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logotipopreventix.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/iconpreventix.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D1B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suaempresa.preventix",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Este aplicativo requer acesso à câmera para tirar fotos.",
        "NSPhotoLibraryUsageDescription": "Este aplicativo requer acesso à sua biblioteca de fotos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/IconLogobg.png",
        "backgroundColor": "#0D0D1B"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/IconLogobg.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}